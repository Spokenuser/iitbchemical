              </div>
          
    
          {% endblock %}
            </div>
        </header>
        </section>
        {% if page.highlighted %}
        <div class="highlighted">
          <aside class="{{ container }} section clearfix" role="complementary">
            {{ page.highlighted }}
          </aside>
        </div>
        {% endif %}
        <section class="inside-banner" style="background-image: url('/themes/custom/chem_custom/images/faculty-banner.svg');">
         
            <div class="container-fluid">
            <div class="inside-heading-bradcumb">
             <h2>{{ node.label }}</h2>
              {% if page.breadcrumb %}
              {{ page.breadcrumb }}
              {% else %}
              {# <div class="mt-5"></div> #} <!-- Was included for a space if the breadcrumb is not there for a page -->
              {% endif %}
             
            </div>          
            </div>
         
         
        </section>
        {% if page.featured_top %}
        {% block featured %}
        <div class="featured-top">
          <aside class="featured-top__inner section clearfix" role="complementary">
            {{ page.featured_top }}
          </aside>
        </div>
        {% endblock %}
        {% endif %}
        <div id="main-wrapper" class="layout-main-wrapper">
          {% block content %}
          <div id="main">
            <div class="row row-offcanvas row-offcanvas-left clearfix">
              {% if page.sidebar_first %}
              <div{{ sidebar_first_attributes }}>
                <aside class="section" role="complementary">
                  {{ page.sidebar_first }}
                </aside>
            </div>
            {% endif %}
            <main class="page-body">
              <section class="section">
                <a id="main-content" tabindex="-1"></a>
                {{ page.content }}
              </section>
            </main>
            {% if page.sidebar_second %}
            <div{{ sidebar_second_attributes }}>
              <aside class="section" role="complementary">
                {{ page.sidebar_second }}
              </aside>
          </div>
          {% endif %}
        </div>
      </div>
      {% endblock %}
    </div>
    {% if page.featured_bottom_first %}
    <div class="featured-bottom">
      <aside>
        <div class="featured-bottom-first featured-bottom-first-bg">
          <div class="{{ container }} clearfix" role="complementary">
            {{ page.featured_bottom_first }}
          </div>
        </div>
      </aside>
    </div>
    {% endif %}
    {% if page.featured_bottom_second or page.featured_bottom_third %}
    <div class="featured-bottom-last {% if page.footer_first %}overlapped{% endif %} 
          {% if page.featured_bottom_second and not page.featured_bottom_third %}featured-bottom-second-bg{% endif %}
          {% if page.featured_bottom_third and not page.featured_bottom_second %}featured-bottom-third-bg{% endif %}
          {% if page.featured_bottom_second and page.featured_bottom_third %}featured-bottom-third-bg{% endif %}">
      <aside>
        {% if page.featured_bottom_second %}
        <div class="featured-bottom-second featured-bottom-second-bg">
          <div class="{{ container }} clearfix" role="complementary">
            {{ page.featured_bottom_second }}
          </div>
        </div>
        {% endif %}
        {% if page.featured_bottom_third %}
        <div class="featured-bottom-third featured-bottom-third-bg">
          <div class="{{ container }} clearfix" role="complementary">
            {{ page.featured_bottom_third }}
          </div>
        </div>
        {% endif %}
      </aside>
    </div>
    {% endif %}
    <footer>
      <div class="container-fluid">
        <div class="item">
          <h4>Contact</h4>
          <div class="contact-info-list">
            {{ footer_contact_block }}
          </div>
        </div>
        <div class="item">
          <h4>Activities</h4>
          <div class="footer-link">
            {{ footer_activities_block }}
          </div>
        </div>
        <div class="item">
          <h4>Resources</h4>
          <div class="footer-link">
            {{ footer_resource_block }}
          </div>
          <div class="social-links">
            {{ social_block }}
          </div>
        </div>
      </div>
    </footer>
    </div>
    </div>